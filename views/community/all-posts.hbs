<div id="community-screen">

  <h1><span class="community-title">Community </span>Forum</h1>

  <a href="/community/new">
    <button class="btn">Add Post</button>
  </a>

  <br>
  <br>

  <div id="post-box-container">
  {{#each posts}}
    <div id="post-box">
      <p>{{owner.userName}}</p>
      {{!-- <p>{{profileImage}}</p> --}}
      <p>{{location}}</p>
      <a href="/community/post-details/{{_id}}">
        <img class="event-img" src="{{imageUrl}}" alt="event" />
      </a>
      <h3>{{name}}</h3>
      <p>{{description}}</p>
      {{!-- <hr /> --}}
    </div>
  {{/each}}
  </div>

</div>